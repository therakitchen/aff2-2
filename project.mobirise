            ".icon-video": {
              "font-size": "1.5rem !important",
              "width": "50px",
              "height": "50px",
              "top": "~\"calc(50% - 25px)\"",
              "left": "~\"calc(50% - 25px)\"",
              "&:before": {
                "border-radius": "100px",
                "background": "#444444",
                "width": "100%",
                "height": "100%",
                "padding-left": "6px",
